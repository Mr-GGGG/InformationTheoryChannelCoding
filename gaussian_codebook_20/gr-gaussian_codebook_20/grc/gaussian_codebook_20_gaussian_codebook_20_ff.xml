<?xml version="1.0"?>
<block>
  <name>gaussian_codebook_20_ff</name>
  <key>gaussian_codebook_20_gaussian_codebook_20_ff</key>
  <category>gaussian_codebook_20</category>
  <import>import gaussian_codebook_20</import>
  <make>gaussian_codebook_20.gaussian_codebook_20_ff($mu)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Mu</name>
    <key>mu</key>
    <type>float</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
